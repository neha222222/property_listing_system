{
  "name": "property-listing-system",
  "version": "1.0.0",
  "description": "A robust backend system for managing property listings with advanced features including CRUD operations, user authentication, property favorites, and recommendations.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "mocha -r ts-node/register 'src/tests/**/*.test.ts' --timeout 10000",
    "seed": "ts-node src/scripts/seed.ts"
  },
  "keywords": [
    "property",
    "listing",
    "real-estate",
    "backend",
    "typescript",
    "nodejs",
    "mongodb",
    "redis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "redis": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^5.2.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mocha": "^10.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.29",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "axios": "^1.9.0",
    "chai": "^5.2.0",
    "eslint": "^9.28.0",
    "mocha": "^11.5.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
